class Main2 {
	public static void main (String[] args) {

	Lamp led = new Lamp;
	Lamp halogen = new Lamp;

	led.turnOn();

	halogen.turnOff();
	}
}

class Lamp2 {
	public static void main(String[] args) {

	boolean isOn;

	void turnOn() {
		isOn = true;
		System.out.println("Light on? " + ison);
}

	void turnOff () {
		isOn = false;
		System.out.println("Light on? " + ison);
	};
	}
}